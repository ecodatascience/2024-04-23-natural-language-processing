@article{ WOS:000396377500012,
Author = {Kluvankova, Tatiana and Gezik, Veronika},
Title = {Survival of commons? Institutions for robust forest social - ecological
   systems},
Journal = {JOURNAL OF FOREST ECONOMICS},
Year = {2016},
Volume = {24},
Pages = {175-185},
Month = {AUG},
Abstract = {In recent decades, human activities have moved beyond the range of
   natural variability and are approaching critical tipping points that may
   lead to irreversible changes to the Earth's systems. In particular, the
   diversity of actors and scales, and their power and interest in Earth
   system resources, increases natural social interconnectivity and the
   vulnerability of these traditionally local resource systems to
   disturbances. Using a combination of design conditions and robustness
   analyses, we argue that institutional maturity and local knowledge of
   self-organised regimes are pre-conditions for the continuity of local
   forest socio-ecological systems as long-lasting institutions that
   survive global market disturbances. Vulnerability and robustness against
   external natural and social disturbances thus largely depend on
   institutional robustness, as well as socioecological dynamics. (C) 2016
   Department of Forest Economics, Swedish University of Agricultural
   Sciences, Umea. Published by Elsevier GmbH. All rights reserved.},
Publisher = {ELSEVIER GMBH},
Address = {HACKERBRUCKE 6, 80335 MUNICH, GERMANY},
Type = {Article},
